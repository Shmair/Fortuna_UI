{
  "name": "refunds-platform",
  "version": "1.0.0",
  "description": "Insurance refunds platform with comprehensive testing",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--no-deprecation react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest --config tests/jest.config.js",
    "test:unit": "jest --config tests/jest.config.js --testPathPattern=tests/unit",
    "test:integration": "jest --config tests/jest.config.js --testPathPattern=\"integration\" --maxWorkers=2",
    "test:e2e": "playwright test --config tests/playwright.config.js",
    "test:e2e:smoke": "playwright test --config tests/playwright.config.js tests/e2e/smoke-refund-flow.spec.js",
    "test:e2e:smoke:headed": "playwright test --config tests/playwright.config.js tests/e2e/smoke-refund-flow.spec.js --headed",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:setup": "node tests/setup/setup-test-db.js",
    "test:cleanup": "node tests/setup/cleanup-test-data.js",
    "test:supabase": "node tests/database/simple-supabase-test.js",
    "test:supabase:sql": "node tests/database/run-supabase-tests.js",
    "test:supabase:final": "node tests/database/run-supabase-tests.js --test=final",
    "test:supabase:rls": "node tests/database/run-supabase-tests.js --test=rls",
    "test:supabase:invoker": "node tests/database/run-supabase-tests.js --test=invoker",
    "test:supabase:verbose": "node tests/database/run-supabase-tests.js --verbose",
    "test:profile": "node tests/run-profile-tests.js",
    "test:profile:comprehensive": "jest --config tests/jest.config.js tests/integration/api/comprehensive-profile-api.test.js --verbose",
    "test:language": "node test-language-detection.js",
    "test:language:unit": "jest tests/unit/language-detection.test.js",
    "test:metadata": "jest tests/unit/metadata-structure.test.js",
    "test:refund-flow": "node tests/scripts/refund-flow-test-runner.js",
    "test:refund-flow:api": "node tests/scripts/refund-flow-test-runner.js --api",
    "test:refund-flow:security": "node tests/scripts/refund-flow-test-runner.js --security",
    "test:refund-flow:performance": "node tests/scripts/refund-flow-test-runner.js --performance",
    "test:refund-flow:report": "node tests/scripts/refund-flow-test-runner.js --report",
    "test:fast": "jest --config tests/jest.config.js --testPathPattern=\"unit\" --maxWorkers=4",
    "test:core": "jest --config tests/jest.config.js --testPathPattern=\"integration/api/(cases|documents|profile)\" --maxWorkers=2",
    "test:ide": "jest --config tests/jest.config.js --testPathPattern=\"integration/api\" --testPathIgnorePatterns=\"enhanced-api-integration|comprehensive-profile-api\" --maxWorkers=2 --verbose --no-coverage",
    "test:cases-api": "jest --config tests/jest.config.js tests/integration/api/cases-api.test.js",
    "test:documents-api": "jest --config tests/jest.config.js tests/integration/api/documents-api.test.js",
    "test:submission-api": "jest --config tests/jest.config.js tests/integration/api/submission-api.test.js",
    "test:chat-integration": "jest --config tests/jest.config.js tests/integration/api/chat-integration.test.js",
    "prebackend:start": "node scripts/kill-port.js 4000",
    "kill-port": "node scripts/kill-port.js",
    "regenerate-embeddings": "node scripts/regenerate-embeddings.js",
    "backend:start": "cd backend && node index.js",
    "backend:test": "jest --config tests/jest.config.js --testPathPattern=tests/integration/api",
    "lint": "eslint src backend",
    "format": "prettier --write \"src/**/*.{js,jsx}\" \"backend/**/*.js\"",
    "deploy:setup": "node scripts/deploy-setup.js",
    "deploy:check": "npm run lint && npm run test:unit && npm run test:integration && npm run build",
    "deploy:keys": "node scripts/generate-keys.js"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@supabase/supabase-js": "^2.0.0",
    "bull": "^4.12.0",
    "canvas": "^2.11.2",
    "express": "^4.18.2",
    "framer-motion": "^12.23.22",
    "franc": "^6.2.0",
    "ioredis": "^5.3.2",
    "jsonrepair": "^3.13.1",
    "lucide-react": "latest",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.3",
    "pdf2pic": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.17.0",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^5.0.1",
    "sharp": "^0.33.0",
    "sonner": "^2.0.7",
    "source-map-loader": "^5.0.0",
    "tesseract.js": "^5.0.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@playwright/test": "^1.40.0",
    "@supabase/mcp-server-supabase": "^0.5.8",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "autoprefixer": "^10.4.22",
    "babel-jest": "^29.7.0",
    "cross-env": "^10.1.0",
    "dotenv": "^16.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pg": "^8.11.3",
    "postcss": "^8.5.6",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.13",
    "zod-to-json-schema": "^3.24.6"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
