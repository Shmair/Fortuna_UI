{
  "name": "refunds-platform",
  "version": "1.0.0",
  "description": "Insurance refunds platform with comprehensive testing",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest --config tests/jest.config.js",
    "test:unit": "jest --config tests/jest.config.js --testPathPattern=tests/unit",
    "test:integration": "jest --config tests/jest.config.js --testPathPattern=tests/integration",
    "test:e2e": "playwright test --config tests/playwright.config.js",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:setup": "node tests/setup/setup-test-db.js",
    "test:cleanup": "node tests/setup/cleanup-test-data.js",
    "test:supabase": "node tests/database/simple-supabase-test.js",
    "test:supabase:sql": "node tests/database/run-supabase-tests.js",
    "test:supabase:final": "node tests/database/run-supabase-tests.js --test=final",
    "test:supabase:rls": "node tests/database/run-supabase-tests.js --test=rls",
    "test:supabase:invoker": "node tests/database/run-supabase-tests.js --test=invoker",
    "test:supabase:verbose": "node tests/database/run-supabase-tests.js --verbose",
    "backend:start": "cd backend && node index.js",
    "backend:test": "jest --config tests/jest.config.js --testPathPattern=tests/integration/api",
    "lint": "eslint src backend",
    "format": "prettier --write \"src/**/*.{js,jsx}\" \"backend/**/*.js\""
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@supabase/supabase-js": "^2.0.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.17.0",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^5.0.1",
    "sonner": "^2.0.7",
    "source-map-loader": "^5.0.0",
    "webpack-dev-server": "^5.2.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pg": "^8.11.3",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
